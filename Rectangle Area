#include <iostream>

using namespace std;
class Rectangle
{
  protected:
  int width,height;
  public:
  void display()
  {
  cout<<width<<" "<<height;
  }
};
class RectangleArea:public Rectangle
{
  public:
  void read_input()
  {
    cin>>width>>height;
  }
  void display()
  {
    cout<<endl<<width*height;
  }
};
